var imgSrc = {
    "groupImg": "data:image/jpeg;base64,",
    
    "userImg" : "data:image/png;base64,"
    
}

var devices = require("device")
var results = devices.query({fields:"*"})
var devices = results.result.devices;

var myDevices = [];

for(var i = 0; i < devices.length; i++){
    var user = {};
    user["user"] = devices[i].id[0]; 
    user["description"] = "user"; 
    user["code"] = devices[i].id[0]; 
    user["pic"] = imgSrc.userImg;
    user["icon"] = "fa fa-user";    
    myDevices.push(user);
}

var groups =  apsdb.callApi("ListGroups", null, null).result.groups;

for(var y = 0; y < groups.length; y++){
    var group = {};
    group["user"] = groups[y].name;
    group["code"] = groups[y].name; 
    group["description"] = "group"; 
    group["pic"] = imgSrc.groupImg;
    group["icon"] = "fa fa-group";   
    myDevices.push(group);
}

myDevices.push({
    "user" : "anonymous",
    "description" : "group",
    "code" : "anonymous",
    "icon" : "fa fa-group",
    "pic" : imgSrc.groupImg
})

myDevices.push({
    "user" : "authenticated",
    "description" : "group",
    "code" : "authenticated",
    "icon" : "fa fa-group",
    "pic" : imgSrc.groupImg
})

myDevices.push({
    "user" : "scriptr",
    "description" : "group",
    "code" : "scriptr",
    "icon" : "fa fa-group",
    "pic" : imgSrc.groupImg
})

return myDevices